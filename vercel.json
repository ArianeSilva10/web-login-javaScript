{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(css|js|png|jpg|gif|svg|ico))",
        "dest": "public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "public/index.html"
      }
    ]
  }